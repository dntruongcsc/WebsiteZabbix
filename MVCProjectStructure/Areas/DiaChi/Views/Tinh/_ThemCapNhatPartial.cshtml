@*@model MVCProjectStructure.Areas.DiaChi.Models.TinhModel.Output.ThongTinTinhWeb*@
<input id="IdEdit" hidden value="@Model.Id" />
<input id="KetQuaOutput" data-ketqua="@ViewBag.KetQua" hidden />
<div id="ThongTinTinhBody">
    <div class="clearfix"></div>
    <div class="font-small row p-3">
        <div class="form-group col-12">
            <h6 class="p-1 text-info ">Thông tin chi tiết</h6>
            <hr class="my-1 mb-1">
        </div>
        <div class="col-md-6 col-12">
            <div class="col-12 p-0">
                <div class="row p-0 m-0 mb-1">
                    <label class="col-sm-3 float-left">Tên tỉnh</label>
                    <div class="col-sm-9 col-9 col-md-9 float-left input-group input-group-sm p-0">
                        <input id="txtTenTinh" type="text" class="form-control mb-1" aria-label="Small" placeholder="Nhập tên tỉnh" value="@Model.Ten">
                    </div>
                </div>
            </div>
        </div>
        <div class="col-md-6 col-12">
            <div class="col-12 p-0">
                <div class="row p-0 m-0 mb-1">
                    <label class="col-sm-3 float-left">Mã tỉnh</label>
                    <div class="col-sm-9 col-9 col-md-9 float-left input-group input-group-sm p-0">
                        <input id="txtMaTinh" type="text" class="form-control mb-1" aria-label="Small" placeholder="Nhập mã tỉnh" value="@Model.Ma">
                    </div>
                </div>
            </div>
            <div class="col-12 p-0">
                <label class="col-sm-3 float-left">&nbsp;</label>
                <div class="col-sm-8 float-left p-0 ml-3">
                    <div class="form-check form-check-inline">
                        <input id="txtTinhHienTai" @((Model.TinhHienTai) ? "checked" : "" ) class="form-check-input" type="checkbox">
                        <label class="form-check-label" for="txtTinhHienTai">Tỉnh hiện tại</label>
                    </div>
                </div>
                <div class="clearfix"></div>
            </div>
        </div>
    </div>
</div>
<script>
        $(document).ready(function () {
            var Jsoninputdata = {
                TuKhoa: ""
            };
        });
</script>


