    <link href="~/Template/front-end/css/easy-responsive-tabs.css" rel="stylesheet" />
    <link href="~/Template/front-end/css/nncm.css" rel="stylesheet" />
    <link href="~/Template/front-end/css/owl.carousel.css" rel="stylesheet" />
    <link href="~/Template/front-end/css/owl.theme.css" rel="stylesheet" />
    <link href="~/Template/front-end/css/owl.theme.default.min.css" rel="stylesheet" />
